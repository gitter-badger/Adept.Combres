<?xml version="1.0" encoding="utf-8" ?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Adept.Combres</id>
    <version>$version$</version>
    <authors>Paul Knopf</authors>
    <description>A Combres filter that builds .less files in a way that you would expect. You can use tildies or use relative selectors for imports. This is ideal for large css frameworks such as Twitter Bootstrap which has 10-15+ .less files.</description>
    <language>en-US</language>
    <projectUrl>http://pknopf.com/blog/using-less-correctly-with-combres</projectUrl>
    <licenseUrl>http://opensource.org/licenses/mit-license.php</licenseUrl>
    <dependencies>
      <dependency id="combres" version="2.2.2.15" />
    </dependencies>
  </metadata>
  <files>
    <file src="readme.txt" target="readme.txt"/>
    <file src="Adept.Combres.dll" target="lib" />
    <file src="Adept.Combres.dll" target="lib" />
  </files>
</package>